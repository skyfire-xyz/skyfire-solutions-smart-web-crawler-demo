{
  "name": "crawler-bot-protection",
  "version": "1.0.0",
  "description": "Node.js/Express proxy service with Redis-based bot protection and usage tracking",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "postbuild": "echo 'Build completed successfully'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": ["proxy", "bot-protection", "redis", "express", "typescript"],
  "author": "kmiyauchi",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.6.1",
    "jose": "^6.0.11",
    "morgan": "^1.10.0",
    "typescript": "^5.8.3",
    "esbuild": "^0.20.0",
    "@esbuild/darwin-arm64": "^0.20.0",
    "tsx": "^4.20.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  }
}
