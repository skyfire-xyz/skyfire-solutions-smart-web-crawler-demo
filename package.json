{
  "name": "crawler-bot-protection",
  "version": "1.0.0",
  "description": "Node.js/Express proxy service with Redis-based bot protection and usage tracking",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "build": "tsc",
    "postbuild": "echo 'Build completed successfully'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "proxy",
    "bot-protection",
    "redis",
    "express",
    "typescript"
  ],
  "author": "kmiyauchi",
  "license": "ISC",
  "dependencies": {
    "@types/datadog-winston": "^1.0.7",
    "cors": "^2.8.5",
    "datadog-winston": "^1.6.0",
    "dd-trace": "^5.58.0",
    "dotenv": "^17.0.1",
    "esbuild": "^0.20.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.6.1",
    "jose": "^6.0.11",
    "morgan": "^1.10.0",
    "pino": "^9.7.0",
    "pino-datadog-transport": "^3.0.6",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.10",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.1.1"
  }
}
